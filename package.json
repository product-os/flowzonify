{
  "name": "flowzonify",
  "version": "1.0.0",
  "description": "A CLI utility to make switching to flowzone more automated",
  "type": "module",
  "bin": {
    "flowzonify": "./dist/index.js"
  },
  "scripts": {
    "lint": "balena-lint src && prettier --check **/*.json **/*.yml && tsc --noEmit --project .",
    "lint-fix": "balena-lint --fix src && prettier -w **/*.json **/*.yml && tsc --noEmit --project .",
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "npm run clean && tsc --project ./tsconfig.build.json",
    "clean": "rimraf dist/",
    "prepack": "npm run build",
    "posttest": "npm run lint",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\" && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/product-os/flowzonify.git"
  },
  "keywords": [
    "ci",
    "flowzone"
  ],
  "author": "Thodoris Greasidis <thodoris@balena.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/product-os/flowzonify/issues"
  },
  "homepage": "https://github.com/product-os/flowzonify#readme",
  "versionist": {
    "publishedAt": "2022-12-27T03:14:37.588Z"
  },
  "dependencies": {
    "@humanwhocodes/env": "^2.2.2",
    "@octokit/rest": "^19.0.5",
    "got": "^12.5.3",
    "lodash": "^4.17.21",
    "simple-git": "^3.15.1",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.1",
    "@types/got": "^9.6.12",
    "@types/lodash": "^4.14.191",
    "@types/yaml": "^1.9.7",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  }
}
